{
  "expo": {
    "name": "ReHomeBud",
    "slug": "ReHomeBud",
    "scheme": "rehomebud",
    "icon": "./assets/raw.png",
    "splash": {
      "image": "./assets/raw.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.camille3.rehomebud",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/raw.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/raw.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {}
    }
  }
}
